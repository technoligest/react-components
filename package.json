  "devDependencies": {
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "awesome-phonenumber": "^2.72.0",
    "classnames": "^2.3.1",
    "luxon": "^2.3.2",
    "react-icons": "^4.3.1",
